{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",

  "properties": {
    "objects": {
      "type": "object",
      "properties": {
        "edit": {
          "type": "string",
          "enum": [".txt"]
        }
      },
      "required": ["edit"],
      "additionalProperties": false
    },

    "payload": {
      "type": "object",
      "properties": {
        "trace": {
          "type": "string"
        }
      },
      "required": ["trace"],
      "additionalProperties": false
    }
  },

  "required": ["objects", "payload"],
  "additionalProperties": false
}
